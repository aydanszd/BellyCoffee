{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "desc": {
      "type": "string"
    },
    "price": {
      "type": "biginteger"
    },
    "oldprice": {
      "type": "biginteger"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "OutOfStock": {
      "type": "boolean"
    },
    "product_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "product_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-tag.product-tag",
      "inversedBy": "products"
    },
    "product_colors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-color.product-color",
      "inversedBy": "products"
    },
    "FeaturedProducts": {
      "type": "boolean"
    },
    "HomePage": {
      "type": "boolean"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "products"
    }
  }
}
